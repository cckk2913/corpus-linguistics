mages/bc8ee5de-c161-428d-bb01-5d65cb350de7/full.jpeg","normalizedUrl":"https://megapx.dcard.tw/v1/images/bc8ee5de-c161-428d-bb01-5d65cb350de7","thumbnail":"https://megapx-assets.dcard.tw/images/bc8ee5de-c161-428d-bb01-5d65cb350de7/640.jpeg","type":"image/megapx","tags":["ANNOTATED"],"createdAt":"2020-01-09T12:48:33.607Z","updatedAt":"2020-01-12T13:07:30.131Z","width":800,"height":450},{"id":"8ca1e599-f6ea-48b0-9b09-5bc55b082bc2","url":"https://megapx-assets.dcard.tw/images/fc1cbeae-3cc3-4496-8d86-1eb33a22676f/full.jpeg","normalizedUrl":"https://megapx.dcard.tw/v1/images/fc1cbeae-3cc3-4496-8d86-1eb33a22676f","thumbnail":"https://megapx-assets.dcard.tw/images/fc1cbeae-3cc3-4496-8d86-1eb33a22676f/320.jpeg","type":"image/megapx","tags":["ANNOTATED"],"createdAt":"2020-01-09T12:48:33.607Z","updatedAt":"2020-01-12T13:07:30.131Z","width":640,"height":360},{"id":"2cdc23c1-5c09-4f45-8e1a-224a5d255bc5","url":"https://megapx-assets.dcard.tw/images/e4c61427-7b62-474c-b58e-30f3210a3b94/full.jpeg","normalizedUrl":"https://megapx.dcard.tw/v1/images/e4c61427-7b62-474c-b58e-30f3210a3b94","thumbnail":"https://megapx-assets.dcard.tw/images/e4c61427-7b62-474c-b58e-30f3210a3b94/320.jpeg","type":"image/megapx","tags":["ANNOTATED"],"createdAt":"2020-01-09T12:48:33.607Z","updatedAt":"2020-01-12T13:07:30.131Z","width":512,"height":288},{"id":"c2f20b40-493f-4498-8818-d1f834e87723","url":"https://megapx-assets.dcard.tw/images/33bc986a-1117-4e88-9ced-cec89efd6a48/full.jpeg","normalizedUrl":"https://megapx.dcard.tw/v1/images/33bc986a-1117-4e88-9ced-cec89efd6a48","thumbnail":"https://megapx-assets.dcard.tw/images/33bc986a-1117-4e88-9ced-cec89efd6a48/320.jpeg","type":"image/megapx","tags":["ANNOTATED"],"createdAt":"2020-01-09T12:48:33.607Z","updatedAt":"2020-01-12T13:07:30.131Z","width":540,"height":392},{"id":"4b62b14f-9418-4b64-9b2b-ea0c6dced34c","url":"https://megapx-assets.dcard.tw/images/85adf6a7-d525-49af-a29d-7758281349b2/full.jpeg","normalizedUrl":"https://megapx.dcard.tw/v1/images/85adf6a